{
  "name": "@spartacus/smartedit",
  "version": "6.7.0",
  "description": "Smart Edit feature library for Spartacus",
  "keywords": [
    "spartacus",
    "framework",
    "storefront",
    "smartedit"
  ],
  "homepage": "https://github.com/SAP/spartacus",
  "repository": "https://github.com/SAP/spartacus/tree/develop/feature-libs/smartedit",
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@spartacus/core": "6.7.0",
    "@spartacus/schematics": "6.7.0",
    "rxjs": "^6.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./schematics/collection.json",
  "module": "fesm2015/spartacus-smartedit.mjs",
  "es2020": "fesm2020/spartacus-smartedit.mjs",
  "esm2020": "esm2020/spartacus-smartedit.mjs",
  "fesm2020": "fesm2020/spartacus-smartedit.mjs",
  "fesm2015": "fesm2015/spartacus-smartedit.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/spartacus-smartedit.mjs",
      "es2020": "./fesm2020/spartacus-smartedit.mjs",
      "es2015": "./fesm2015/spartacus-smartedit.mjs",
      "node": "./fesm2015/spartacus-smartedit.mjs",
      "default": "./fesm2020/spartacus-smartedit.mjs"
    },
    "./core": {
      "types": "./core/index.d.ts",
      "esm2020": "./esm2020/core/spartacus-smartedit-core.mjs",
      "es2020": "./fesm2020/spartacus-smartedit-core.mjs",
      "es2015": "./fesm2015/spartacus-smartedit-core.mjs",
      "node": "./fesm2015/spartacus-smartedit-core.mjs",
      "default": "./fesm2020/spartacus-smartedit-core.mjs"
    },
    "./root": {
      "types": "./root/index.d.ts",
      "esm2020": "./esm2020/root/spartacus-smartedit-root.mjs",
      "es2020": "./fesm2020/spartacus-smartedit-root.mjs",
      "es2015": "./fesm2015/spartacus-smartedit-root.mjs",
      "node": "./fesm2015/spartacus-smartedit-root.mjs",
      "default": "./fesm2020/spartacus-smartedit-root.mjs"
    }
  },
  "sideEffects": false
}