    "through2": "^3.0.1",
    "ts-jest": "^26.4.4",
    "ts-morph": "^9.1.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2",
    "webpack-cli": "^3.3.12"
  }
}
