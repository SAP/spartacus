    "cy:open:b2b": "cypress open --env BASE_SITE=powertools-spa,OCC_PREFIX_USER_ENDPOINT=orgUsers",
    "cy:open:epd-visualization": "cypress open --env BASE_SITE=powertools-epdvisualization-spa",
    "cy:open:ccv2": "cypress open --config baseUrl=https://spartacusstore.cg79x9wuu9-eccommerc1-p1-public.model-t.myhybris.cloud --env API_URL=https://api.cg79x9wuu9-eccommerc1-p1-public.model-t.myhybris.cloud",
    "cy:open:mcs": "cypress open --config baseUrl=https://spartacusstore.cg79x9wuu9-ecdhcomme2-s1-public.model-t.myhybris.cloud --env API_URL=https://api.cg79x9wuu9-ecdhcomme2-s1-public.model-t.myhybris.cloud",
