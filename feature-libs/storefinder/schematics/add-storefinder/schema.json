{
  "$schema": "http://json-schema.org/schema",
  "id": "StorefinderSchematics",
  "title": "Storefinder Schematics",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "lazy": {
      "type": "boolean",
      "description": "Lazy load the storefinder features.",
      "default": true
    },
    "configuration": {
      "description": "The Spartacus configuration",
      "type": "string",
      "default": "b2c"
    }
  },
  "required": []
}
