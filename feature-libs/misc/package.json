{
  "name": "@spartacus/misc",
  "version": "3.0.0-next.6",
  "description": "Miscellaneous feature library for Spartacus",
  "homepage": "https://github.com/SAP/spartacus",
  "keywords": [
    "spartacus",
    "framework",
    "storefront",
    "storefinder",
    "misc"
  ],
  "scripts": {
    "clean:schematics": "rimraf \"schematics/**/*.js\" \"schematics/**/*_spec.js\" \"schematics/**/*.js.map\" \"schematics/**/*.d.ts\"",
    "build:schematics": "yarn clean:schematics && tsc -p ./tsconfig.schematics.json",
    "test:schematics": "yarn clean:schematics && tsc -p ./tsconfig.schematics.spec.json && jasmine \"schematics/**/*_spec.js\""
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/SAP/spartacus/tree/develop/feature-libs/misc",
  "schematics": "./schematics/collection.json",
  "peerDependencies": {
    "@angular/common": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/router": "^10.1.0",
    "rxjs": "^6.6.0",
    "@spartacus/core": "3.0.0-next.6",
    "@spartacus/storefront": "3.0.0-next.6",
    "@spartacus/schematics": "3.0.0-next.6"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "@types/node": "^12.11.1",
    "@types/shelljs": "^0.8.7",
    "jasmine": "^3.5.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "typescript": "~4.0.2"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
